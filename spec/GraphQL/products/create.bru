meta {
  name: create
  type: graphql
  seq: 3
}

post {
  url: https://api.escuelajs.co/graphql
  body: graphql
  auth: none
}

body:graphql {
  mutation {
  	addProduct(
  		data: {
  			title: "New Product"
  			price: 10
  			description: "A description"
  			categoryId: 1
  			images: ["https://placeimg.com/640/480/any"]
  		}
  	) {
  		title
  		price
  		images
  		category {
  			id
  			name
  			image
  		}
  	}
  }
  
}
